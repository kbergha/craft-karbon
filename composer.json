{
    "name": "kbergha/karbon",
    "description": "Brings the power of date / datetime handling from Carbon to Twig, hopefully helping to answer the age old question of: what (date)-time is it?",
    "type": "craft-plugin",
    "version": "0.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "karbon"
    ],
    "support": {
        "docs": "https://github.com/kbergha/craft-karbon/blob/master/README.md",
        "issues": "https://github.com/kbergha/craft-karbon/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Knut Erik Berg-Hansen",
            "homepage": "https://github.com/kbergha"
        }
    ],
    "require": {
        "craftcms/cms": "^3.1.0"
    },
    "autoload": {
        "psr-4": {
          "kbergha\\karbon\\": "src/"
        }
    },
    "extra": {
        "name": "karbon",
        "handle": "karbon",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/kbergha/craft-karbon/master/CHANGELOG.md",
        "class": "kbergha\\karbon\\Karbon"
    }
}
